@model CRM_CUS.Models.Isocurrency

@{
    ViewData["Title"] = "Delete";
}

<h1>Delete</h1>

<h3>Are you sure you want to delete this?</h3>
<div>
    <h4>Isocurrency</h4>
    <hr />
        <dl class="row">
        <dt class = "col-sm-2">
                @Html.DisplayName("Валюта")
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.NameCurrency)
        </dd>
        <dt class = "col-sm-2">
                @Html.DisplayName("Код валюты")
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.CodeCurrency)
        </dd>
        <dt class = "col-sm-2">
                @Html.DisplayName("Тип валюты")
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.DescriptionRu)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Удалить" class="btn btn-danger" /> |
        <a asp-action="Index">Вернуться</a>
    </form>
</div>
